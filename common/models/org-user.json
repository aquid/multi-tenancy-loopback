{
  "name": "orgUser",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "orgRoles": {
      "type": "hasMany",
      "model": "orgRole",
      "foreignKey": "orgUserId",
      "through": "orgRoleMapping"
    },
    "organisation": {
      "type": "belongsTo",
      "model": "organisation",
      "foreignKey": "orgId"
    },
    "items": {
      "type": "hasMany",
      "model": "items",
      "foreignKey": "creatorId"
    },
    "orgRoleMappings": {
      "type": "hasMany",
      "model": "orgRoleMapping",
      "foreignKey": "orgUserId"
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "create"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    }
  ],
  "methods": {}
}
